{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "rustman",
  "version": "0.1.0",
  "identifier": "com.rustman.dev",
  "build": {
    "beforeBuildCommand": "cd src-yew || cd ../src-yew && trunk build --release",
    "beforeDevCommand": "cd src-yew || cd ../src-yew && trunk serve --port 1420",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../src-yew/dist"
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "rustman",
        "width": 768,
        "minWidth": 768,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null,
      "capabilities": [
        "default"
      ]
    },
    "withGlobalTauri": true
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "copyright": "",
    "externalBin": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "longDescription": "",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "resources": [],
    "shortDescription": "",
    "targets": "all",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    }
  }
}
