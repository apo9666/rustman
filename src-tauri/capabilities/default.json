{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capabilities for rustman",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-message",
    "dialog:allow-confirm",
    "fs:allow-read-file",
    "fs:allow-read-text-file",
    "fs:allow-exists",
    "fs:allow-write-file",
    "fs:allow-write-text-file",
    "fs:allow-home-read",
    "fs:allow-home-read-recursive",
    "fs:allow-home-write",
    "fs:allow-home-write-recursive"
  ]
}
